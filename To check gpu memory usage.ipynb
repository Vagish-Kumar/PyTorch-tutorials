   "source": [
    "import torch\n",
    "def check_memory():\n",
    "    allocated = torch.cuda.memory_allocated() / (1024 ** 3)  # Convert to GB (bytes>kb>mb)\n",
    "    reserved = torch.cuda.memory_reserved() / (1024 ** 3)    \n",
    "    print(f\"Memory allocated: {allocated:.2f} GB\")\n",
    "    print(f\"Memory reserved: {reserved:.2f} GB\")"
   ]
  }
